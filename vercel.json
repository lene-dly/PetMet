{
    "builds": [
      {
        "src": "PetMet/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9",
          "installCommand": "pip install django"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "PetMet/wsgi.py"
      }
    ]
  }